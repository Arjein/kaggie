{
  "name": "kaggler-extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:webapp": "vite --mode webapp",
    "build": "tsc -b && vite build",
    "build:webapp": "tsc -b && vite build --mode webapp",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:console": "open test-final-functionality.html",
    "test:simple": "vite build && node -e \"console.log('âœ… Simple test: Build successful!')\"",
    "test:env": "node -e \"const fs=require('fs'); console.log('Environment check:', fs.readFileSync('../.env', 'utf8').split('\\n').filter(l=>l.includes('VITE_')).map(l=>l.split('=')[0]+'='+(l.includes('KEY')?'[SET]':'SET')))\"",
    "test:full": "npm run test:env && npm run test:simple && npm run test:console",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@langchain/community": "^0.3.45",
    "@langchain/core": "^0.3.57",
    "@langchain/langgraph": "^0.2.74",
    "@langchain/langgraph-checkpoint-sqlite": "^0.1.5",
    "@langchain/openai": "^0.5.11",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.7",
    "@tavily/core": "^0.5.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "duck-duck-scrape": "^2.2.7",
    "flowbite": "^3.1.2",
    "framer-motion": "^12.16.0",
    "katex": "^0.16.22",
    "lucide-react": "^0.511.0",
    "motion": "^12.16.0",
    "openai": "^4.103.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwindcss": "^4.1.7",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/chrome": "^0.0.323",
    "@types/node": "^22.15.23",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.1.4",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
